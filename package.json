{
  "name": "scss-flex",
  "version": "1.6.2-0",
  "description": "flexbox，移动端 flex 布局神器，兼容微信，UC，webview 等移动端主流浏览器，可以通过 bower 和 npm 下载",
  "main": "dist/scss-flex.css",
  "scripts": {
    "build:patch": "gulp bump:patch && gulp dev",
    "build:prerelease": "gulp bump:prerelease && gulp dev",
    "build:minor": "gulp bump:minor && gulp dev",
    "build:major": "gulp bump:major && gulp dev",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "gulp dev"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vxhly/scss-flex.git"
  },
  "keywords": [
    "scss-flex",
    "flex-scss",
    "flex",
    "scss",
    "sass",
    "flex-sass",
    "sass-flex",
    "flexbox",
    "flex.css",
    "vue-flex",
    "css-flex",
    "flexibule"
  ],
  "author": "vxhly <pengchengou@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vxhly/scss-flex/issues"
  },
  "homepage": "https://github.com/vxhly/scss-flex#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "cz-conventional-changelog": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-bump": "^3.1.0",
    "gulp-cssbeautify": "^1.0.0",
    "gulp-csscomb": "^3.0.8",
    "gulp-cssnano": "^2.1.2",
    "gulp-header": "^2.0.5",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sequence": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "publish": "^0.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
